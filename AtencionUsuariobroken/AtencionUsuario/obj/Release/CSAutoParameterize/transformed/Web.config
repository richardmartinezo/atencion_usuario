	<connectionStrings>
		<add name="DefaultConnection" connectionString="$(ReplacableToken_DefaultConnection-Web.config Connection String_0)" providerName="System.Data.SqlClient" />
		<add name="iFISConnectionString" connectionString="$(ReplacableToken_iFISConnectionString-Web.config Connection String_0)" providerName="System.Data.SqlClient" />
	</connectionStrings>
	<system.web>
		<compilation strict="false" explicit="true" targetFramework="4.0">
